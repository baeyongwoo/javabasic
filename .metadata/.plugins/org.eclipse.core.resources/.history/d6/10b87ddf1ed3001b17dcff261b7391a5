package superclass;

public class Main2 {
	public static void main(String[] args) {
		SuperSonicAirplane ssap = new SuperSonicAirplane(800);
		ssap.fly();
		System.out.println("--------------");
		ssap.fly();
		System.out.println("--------------");
		ssap.breakSpeed();
		ssap.breakSpeed();
		ssap.breakSpeed();
		System.out.println("--------------");
		ssap.fly();
		
	}
}
