package accessmodifier.game;

public class Commoner {
	private  int level;
	private int exp;
	private int hp;
	private int mp;
	
	
	public Commoner(int level, int exp, int hp, int mp) {
		super();
		this.level = level;
		this.exp = exp;
		this.hp = hp;
		this.mp = mp;
	}
	
	public int getLevel() {
		return level;
	}
	public void setLevel(int level) {
		this.level = level;
	}
	public int getExp() {
		return exp;
	}
	public void setExp(int exp) {
		this.exp = exp;
	}
	public int getHp() {
		return hp;
	}
	public void setHp(int hp) {
		this.hp = hp;
	}
	public int getMp() {
		return mp;
	}
	public void setMp(int mp) {
		this.mp = mp;
	}
	
	
}
