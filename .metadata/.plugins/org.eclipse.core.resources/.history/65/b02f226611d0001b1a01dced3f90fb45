public class ForPractice {
	public static void main(String[] args) {
		//		1 6 10 13 15 	i = 1일때 j 5 4 3 2 감소
		//		2 7 11 14 		i = 2일때 j 5 4 3
		//		3 8 12 			i = 3일때 j 5 4
		//		4 9 			i = 4일때 j 5
		//		5
		int arr[][] = new int[5][5];
		int number = 1;
		for(int i = 1; i <= 4; i++) {
			for(int j = 1; j <= 4; j++) {
				arr[i][j] += number;
				number++;
				System.out.println();
			}
			
		}
		
		for(int i = 1; i <= 4; i++) {
			for(int j = 1; j <= 4; j++) {
				System.out.print(arr[i][j]);
			}
			
		}
		
		
	}
}
