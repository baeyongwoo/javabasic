package thiskeyword;
public class Person {
	//사람의 정보를 여기에 기입
	public String name;
	public int age;
	public int money;
	public String major;
	
	public Person(int age, int money, java.lang.String major, pu string) {
		super();
		this.age = age;
		this.money = money;
		this.major = major;
		
	}

	//생성자를 활용


}
