package Interaction.game;

public class User {
	private int hp;
	private int mp;
	private int atk;
	private int def;
	private int lv;
	private int exp;
	public User(int hp, int mp, int atk, int def, int lv, int exp) {
		
		this.hp = hp;
		this.mp = mp;
		this.atk = atk;
		this.def = def;
		this.lv = lv;
		this.exp = exp;
	}
	
	
}
