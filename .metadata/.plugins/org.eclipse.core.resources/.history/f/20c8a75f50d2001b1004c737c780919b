package constructor;

public class Main2 {
	public static void main(String[] args) {
		Student st1 = new Student("김철수", 23, 56);
		Student st2 = new Student("홍길동", 20, 77);
		System.out.println("===========점수 조회 ================");
		st1.getInfo();
		st2.getInfo();
		
		
		
		System.out.println("============합격 여부 ================");
		if(st1.getScore() > 60) {
			System.out.println("합격입니다");
		}System.out.println("불합격입니다");
		
	}
}
