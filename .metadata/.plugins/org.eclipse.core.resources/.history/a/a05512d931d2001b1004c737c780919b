package structure;

public class Structure2 {

	public  static void getInfo(Person p) {
		System.out.println(p.name);
		System.out.println(p.age);
		System.out.println(p.pNum);
		System.out.println(p.address);
		
		
	}
	public static void main(String[] args) {
		Person a = new Person();
		getInfo(a);
	}
}
