import java.awt.Label;

public class break3 {
	public static void main(String[] args) {
		
		for(int i = 2; i <=9; i++) {
			System.out.println(i + "단 출력.");
			
			for(int j =1; j <=9; j++) {
				if(j > 5) {
					System.out.println("*6이상은 직접 알아보기");
					break;
				}
				System.out.println(i + "*" + j + "=" + i*j);
			}
			System.out.println("--------------------");
		}
	}
}
