package Interaction.game;

public class monsterA {
	//몬스터는 체력, 공격력, 방어력 수치
	private int Ahp = 50;
	private int Aatk = 5;
	private int Aamor = 2;
	
	
	
	public monsterA(int ahp, int aatk, int aamor) {
		Ahp = ahp;
		Aatk = aatk;
		Aamor = aamor;
	}
	
	public int getAhp() {
		return Ahp;
	}
	public void setAhp(int ahp) {
		Ahp = ahp;
	}
	public int getAatk() {
		return Aatk;
	}
	public void setAatk(int aatk) {
		Aatk = aatk;
	}
	public int getAamor() {
		return Aamor;
	}
	public void setAamor(int aamor) {
		Aamor = aamor;
	}
	
	
	
}
