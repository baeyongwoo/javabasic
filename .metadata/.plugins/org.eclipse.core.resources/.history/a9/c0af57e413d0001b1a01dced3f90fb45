public class ForPractice {
	public static void main(String[] args) {
		//		1 6 10 13 15 	i = 1일때 j 5 4 3 2 감소
		//		2 7 11 14 		i = 2일때 j 5 4 3
		//		3 8 12 			i = 3일때 j 5 4
		//		4 9 			i = 4일때 j 5
		//		5
		for(int i = 0; i < 5; i++) {
			for(int j =0; j <i + 1; j++) {
				System.out.print(j);
			}
			System.out.println();
		}
		
	}
}
