package accessmodifier;

public class Main {
	public static void main(String[] args) {
		Warrior warrior = new Warrior("zxc213");
		for(int i = 0; i < 3; i++) {
			warrior.hunt();
			warrior.hunt();
			warrior.hunt();
		}
		System.out.println();
		System.out.println(warrior.id + "님의 현재 정보");
		System.out.println("현재 레벨 : " + warrior.level);
		System.out.println("현재 경험치 : " + warrior.exp);
		System.out.println("현재 체력 : " + warrior.hp);
		System.out.println("현재 공격력 : " + warrior.atk);
	}
}
