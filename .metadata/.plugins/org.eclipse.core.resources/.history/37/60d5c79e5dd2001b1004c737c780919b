package constructor;

public class BasketballPlayer {

	public int ki;
	public int jump;
	
	public BasketballPlayer(int ki, int jump) {
		super();
		this.ki = ki;
		this.jump = jump;
	}


	public void dumk() {
		if((ki + jump) >= 300) {
			System.out.println("덩크슛에 성공했습니다.");
		}else System.out.println("골대에 닿지 않았습니다.");
	}
}
