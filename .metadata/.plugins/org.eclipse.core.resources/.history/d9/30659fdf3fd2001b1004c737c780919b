package structure;

public class School {
	//학생정보
	public String name;
	public int age;
	public int score;
	
	public School(String name, int age, int score) {
		super();
		this.name = name;
		this.age = age;
		this.score = score;
	}

	
	public void getInfo() {
		System.out.println("이름 : " + name);
		System.out.println("나이 : " + age);
		System.out.println("점수 : " + score);
	}

}
