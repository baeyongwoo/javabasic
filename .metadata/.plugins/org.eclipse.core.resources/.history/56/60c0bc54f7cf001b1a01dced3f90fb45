import java.util.Scanner;

class n{
	int sum = 0;
	
	public void Sum(int n) {
		for(int i = 1; i < n; i++) {
			if(i%2==0) {
				continue;
			}else sum +=n;
		}
	}
}


public class Continue2 {
	public static void main(String[] args) {
		//1부터 n까지 총합을 구하되 홀수끼리만 더한 총합
		//Continue를 반드시 사용
		
		Scanner scan = new Scanner(System.in);
		int end = scan.nextInt();
	}
}
